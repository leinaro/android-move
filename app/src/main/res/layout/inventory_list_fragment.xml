<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.inventorylist.InventoryListFragment">

  <androidx.recyclerview.widget.RecyclerView
      android:id="@+id/list"
      android:name="com.leinaro.move.BoxListFragment"
      android:layout_width="match_parent"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toBottomOf="@id/add_box_button"
      android:layout_height="match_parent"
      android:layout_margin="16dp"
      app:layoutManager="LinearLayoutManager"
      android:visibility="gone"
      tools:visibility="visible"
      tools:listitem="@layout/fragment_inventory_list_item" />

</FrameLayout>